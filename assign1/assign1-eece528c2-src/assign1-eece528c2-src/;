CC	= mpicc
CFLAGS	= 
LIBS	= 
all	: assignment1 assignment1_O assignment1_O1assignment1_O2 assignment1_O3 assignment1_O2M \
	assignment1_Pivot assignment1_Pivot_O assignment1_Pivot_O1 assignment1_Pivot_O2 assignment1_Pivot_O3 assignment1_Pivot_O2M \
 	assignment1_PivotMPI assignment1_PivotMPI_O assignment1_PivotMPI_O1 assignment1_PivotMPI_O2 assignment1_PivotMPI_O3 assignment1_PivotMPI_OM

clean	:
	rm -f assignment1 assignment1_O assignment1_O1assignment1_O2 assignment1_O3 assignment1_O2M \
        assignment1_Pivot assignment1_Pivot_O assignment1_Pivot_O1 assignment1_Pivot_O2 assignment1_Pivot_O3 assignment1_Pivot_O2M \
        assignment1_PivotMPI assignment1_PivotMPI_O assignment1_PivotMPI_O1 assignment1_PivotMPI_O2 assignment1_PivotMPI_O3 \
	assignment1_PivotMPI_OM  *.o a.out core

assignment1:
	$(CC) -o assignment1 assignment1.c	$(LIBS)

assignment1_O:
	$(CC) -O -o assignment1_O assignment1.c	$(LIBS)

assignment1_O1:
	$(CC) -O1 -o assignment1_O1 assignment1.c	$(LIBS)

assignment1_O2:
	$(CC) -O2 -o assignment1_O2 assignment1.c      $(LIBS)

assignment1_O3:
	$(CC) -O3 -o assignment1_O3 assignment1.c      $(LIBS)

assignment1_O2M:
	$(CC) -O2 -ftree-vectorize -m64 -march=broadwell -mtune=intel -o assignment1_O2M assignment1.c      $(LIBS)

assignment1_Pivot:
	$(CC) -o assignment1_Pivot assignment1_Pivot.c	$(LIBS)

assignment1_Pivot_O:
	$(CC) -O -o assignment1_Pivot_O assignment1_Pivot.c	$(LIBS)

assignment1_Pivot_O1:
	$(CC) -O1 -o assignment1_Pivot_O1 assignment1_Pivot.c	$(LIBS)

assignment1_Pivot_O2:
	$(CC) -O2 -o assignment1_Pivot_O2 assignment1_Pivot.c      $(LIBS)

assignment1_Pivot_O3:
	$(CC) -O3 -o assignment1_Pivot_O3 assignment1_Pivot.c      $(LIBS)

assignment1_Pivot_O2M:
	$(CC) -O2 -ftree-vectorize -m64 -march=broadwell -mtune=intel -o assignment1_O2M assignment1_Pivot.c      $(LIBS)

assignment1_PivotMPI:
	$(CC) -o assignment1_PivotMPI assignment1_PivotMPI.c	$(LIBS)

assignment1_PivotMPI_O:
	$(CC) -O -o assignment1_PivotMPI_O assignment1_PivotMPI.c	$(LIBS)

assignment1_PivotMPI_O1:
	$(CC) -O1 -o assignment1_PivotMPI_O1 assignment1_PivotMPI.c	$(LIBS)

assignment1_PivotMPI_O2:
	$(CC) -O2 -o assignment1_PivotMPI_O2 assignment1_PivotMPI.c      $(LIBS)

assignment1_PivotMPI_O3:
	$(CC) -O3 -o assignment1_PivotMPI_O3 assignment1_PivotMPI.c      $(LIBS)

assignment1_PivotMPI_O2M:
	$(CC) -O2 -ftree-vectorize -m64 -march=broadwell -mtune=intel -o assignment1_PivotMPI_O2M assignment1_PivotMPI.c      $(LIBS)

